#include <stdio.h>
#include <stdlib.h>
#include "mesh.h"
#include "mpi.h"
#include "math.h"

void interpolation_NDFX_1st(Domain *D,External *Ext);
void interpolation_Yee_1st(Domain *D,External *Ext);

void interpolation(Domain *D,External *Ext)
{
  switch((D->fieldType-1)*2+(D->interpolationType-1)) {  
  case ((Yee-1)*2+(FIRST-1)) :
  case ((NoCherenkov-1)*2+(FIRST-1)) :
    interpolation_Yee_1st(D,Ext);	//2D
    break;
  case ((NDFX-1)*2+(FIRST-1)) :
    interpolation_NDFX_1st(D,Ext);	//2D
    break;
  default :
    printf("In interpolation, what interpolationType(%d)?\n",D->interpolationType);
  }
}

void interpolation_NDFX_1st(Domain *D,External *Ext)
{
   int i,j,m,i1,j1,istart,iend,jstart,jend,s,rank,numMode;
   double Bz,Br,Bp,Bx,By,Ez,Er,Ep,Ex,Ey,Pr,Pl,Sr,Sl;
   double coss[D->numMode],sins[D->numMode];
   double extE1,extE2,extE3,extB1,extB2,extB3,x,y,z,R,r,invR;
   double wz1,wz2,wr1,wr2,WZ1,WZ2,WR1,WR2;
   ptclList *p;
   int myrank, nprocs;    

   MPI_Comm_rank(MPI_COMM_WORLD, &myrank);

   istart=D->istart;   iend=D->iend;
   jstart=D->jstart;   jend=D->jend;
   numMode=D->numMode;

   Particle **particle;
   particle=D->particle;

   extE1=Ext->E1;   extE2=Ext->E2;
   extE3=Ext->E3;   extB1=Ext->B1;
   extB2=Ext->B2;   extB3=Ext->B3;

   for(i=istart; i<iend; i++)
     for(j=jstart+1; j<jend; j++) 
     { 
       for(s=0; s<D->nSpecies; s++)
       {
         p=particle[i][j].head[s]->pt;
         while(p) {
           z=p->z;  x=p->x; y=p->y;
           R=sqrt(x*x+y*y); invR=1.0/R;
           r=R+0.5-(j-jstart);

           wz2=z;             wz1=1.0-wz2;
           wr2=r;             wr1=1.0-wr2;
           i1=((int)(i+z+0.5));
           j1=((int)(j+r+0.5));
           WZ2=z+0.5-((int)(z+0.5));  WZ1=1.0-WZ2;
           WR2=r+0.5-((int)(r+0.5));  WR1=1.0-WR2;

           coss[0]=1.0; sins[0]=0.0;
           coss[1]=x*invR; sins[1]=y*invR;
           for(m=2; m<numMode; m++) {
             coss[m]=coss[m-1]*coss[1]-sins[m-1]*sins[1];
             sins[m]=sins[m-1]*coss[1]+coss[m-1]*sins[1];
           }

           Ez=wz1*wr1*D->EzCR[0][i][j]
             +wz2*wr1*D->EzCR[0][i+1][j]
             +wz1*wr2*D->EzCR[0][i][j+1]
             +wz2*wr2*D->EzCR[0][i+1][j+1];
           Bz=wz1*wr1*D->BzCR[0][i][j]
             +wz2*wr1*D->BzCR[0][i+1][j]
             +wz1*wr2*D->BzCR[0][i][j+1]
             +wz2*wr2*D->BzCR[0][i+1][j+1];
           Pr=WZ1*WR1*D->PrCR[0][i1-1][j1-1]
             +WZ2*WR1*D->PrCR[0][i1][j1-1]
             +WZ1*WR2*D->PrCR[0][i1-1][j1]
             +WZ2*WR2*D->PrCR[0][i1][j1];
           Pl=WZ1*WR1*D->PlCR[0][i1-1][j1-1]
             +WZ2*WR1*D->PlCR[0][i1][j1-1]
             +WZ1*WR2*D->PlCR[0][i1-1][j1]
             +WZ2*WR2*D->PlCR[0][i1][j1];
           Sr=WZ1*WR1*D->SrCR[0][i1-1][j1-1]
             +WZ2*WR1*D->SrCR[0][i1][j1-1]
             +WZ1*WR2*D->SrCR[0][i1-1][j1]
             +WZ2*WR2*D->SrCR[0][i1][j1];
           Sl=WZ1*WR1*D->SlCR[0][i1-1][j1-1]
             +WZ2*WR1*D->SlCR[0][i1][j1-1]
             +WZ1*WR2*D->SlCR[0][i1-1][j1]
             +WZ2*WR2*D->SlCR[0][i1][j1];
           for(m=1; m<numMode; m++) {
             Ez+=((wz1*wr1*D->EzCR[m][i][j]
                  +wz2*wr1*D->EzCR[m][i+1][j]
                  +wz1*wr2*D->EzCR[m][i][j+1]
                  +wz2*wr2*D->EzCR[m][i+1][j+1])*coss[m]
                 -(wz1*wr1*D->EzCI[m][i][j]
                  +wz2*wr1*D->EzCI[m][i+1][j]
                  +wz1*wr2*D->EzCI[m][i][j+1]
                  +wz2*wr2*D->EzCI[m][i+1][j+1])*sins[m]);
             Bz+=((wz1*wr1*D->BzCR[m][i][j]
                  +wz2*wr1*D->BzCR[m][i+1][j]
                  +wz1*wr2*D->BzCR[m][i][j+1]
                  +wz2*wr2*D->BzCR[m][i+1][j+1])*coss[m]
                 -(wz1*wr1*D->BzCI[m][i][j]
                  +wz2*wr1*D->BzCI[m][i+1][j]
                  +wz1*wr2*D->BzCI[m][i][j+1]
                  +wz2*wr2*D->BzCI[m][i+1][j+1])*sins[m]);
             Pr+=((WZ1*WR1*D->PrCR[m][i1-1][j1-1]
                  +WZ2*WR1*D->PrCR[m][i1][j1-1]
                  +WZ1*WR2*D->PrCR[m][i1-1][j1]
                  +WZ2*WR2*D->PrCR[m][i1][j1])*coss[m]
                 -(WZ1*WR1*D->PrCI[m][i1-1][j1-1]
                  +WZ2*WR1*D->PrCI[m][i1][j1-1]
                  +WZ1*WR2*D->PrCI[m][i1-1][j1]
                  +WZ2*WR2*D->PrCI[m][i1][j1])*sins[m]);
             Pl+=((WZ1*WR1*D->PlCR[m][i1-1][j1-1]
                  +WZ2*WR1*D->PlCR[m][i1][j1-1]
                  +WZ1*WR2*D->PlCR[m][i1-1][j1]
                  +WZ2*WR2*D->PlCR[m][i1][j1])*coss[m]
                 -(WZ1*WR1*D->PlCI[m][i1-1][j1-1]
                  +WZ2*WR1*D->PlCI[m][i1][j1-1]
                  +WZ1*WR2*D->PlCI[m][i1-1][j1]
                  +WZ2*WR2*D->PlCI[m][i1][j1])*sins[m]);
             Sr+=((WZ1*WR1*D->SrCR[m][i1-1][j1-1]
                  +WZ2*WR1*D->SrCR[m][i1][j1-1]
                  +WZ1*WR2*D->SrCR[m][i1-1][j1]
                  +WZ2*WR2*D->SrCR[m][i1][j1])*coss[m]
                 -(WZ1*WR1*D->SrCI[m][i1-1][j1-1]
                  +WZ2*WR1*D->SrCI[m][i1][j1-1]
                  +WZ1*WR2*D->SrCI[m][i1-1][j1]
                  +WZ2*WR2*D->SrCI[m][i1][j1])*sins[m]);
             Sl+=((WZ1*WR1*D->SlCR[m][i1-1][j1-1]
                  +WZ2*WR1*D->SlCR[m][i1][j1-1]
                  +WZ1*WR2*D->SlCR[m][i1-1][j1]
                  +WZ2*WR2*D->SlCR[m][i1][j1])*coss[m]
                 -(WZ1*WR1*D->SlCI[m][i1-1][j1-1]
                  +WZ2*WR1*D->SlCI[m][i1][j1-1]
                  +WZ1*WR2*D->SlCI[m][i1-1][j1]
                  +WZ2*WR2*D->SlCI[m][i1][j1])*sins[m]);
           }
           Er=(Pr+Pl)*0.5; Bp=(Pr-Pl)*0.5;
           Ep=(Sl+Sr)*0.5; Br=(Sl-Sr)*0.5;
           Ex=Er*coss[1]-Ep*sins[1];
           Ey=Er*sins[1]+Ep*coss[1];
           Bx=Br*coss[1]-Bp*sins[1];
           By=Br*sins[1]+Bp*coss[1];

           p->Ez=Ez; p->Ex=Ex; p->Ey=Ey;
           p->Bz=Bz; p->Bx=Bx; p->By=By;

           p=p->next;
         }
       }		//for(s)        
     }		   //for(i,j)

   j=jstart;
   for(i=istart; i<iend; i++)
     { 
       for(s=0; s<D->nSpecies; s++)
       {
         p=particle[i][j].head[s]->pt;
         while(p) {
           z=p->z;  x=p->x; y=p->y;
           r=sqrt(x*x+y*y); invR=1.0/r;

           coss[0]=1.0; sins[0]=0.0;
           coss[1]=x*invR; sins[1]=y*invR;
           for(m=2; m<numMode; m++) {
             coss[m]=coss[m-1]*coss[1]-sins[m-1]*sins[1];
             sins[m]=sins[m-1]*coss[1]+coss[m-1]*sins[1];
           }

           wz2=z;             wz1=1.0-wz2;
           WZ2=z+0.5-((int)(z+0.5));  WZ1=1.0-WZ2;
           i1=((int)(i+z+0.5));   j1=jstart+1;

           wr2=r*2.0;             wr1=1.0-wr2;
           WR2=r;                 WR1=1.0-WR2;

           m=0;
           Ez=wz1*D->EzCR[m][i][j+1]
             +wz2*D->EzCR[m][i+1][j+1];
           Bz=wz1*D->BzCR[m][i][j+1]
             +wz2*D->BzCR[m][i+1][j+1];
           Pr=WZ1*WR2*D->PrCR[m][i1-1][j1]
             +WZ2*WR2*D->PrCR[m][i1][j1];
           Pl=WZ1*WR2*D->PlCR[m][i1-1][j1]
             +WZ2*WR2*D->PlCR[m][i1][j1];
           Sr=WZ1*WR2*D->SrCR[m][i1-1][j1]
             +WZ2*WR2*D->SrCR[m][i1][j1];
           Sl=WZ1*WR2*D->SlCR[m][i1-1][j1]
             +WZ2*WR2*D->SlCR[m][i1][j1];

           m=1;
           Ez+=((wz1*wr2*D->EzCR[m][i][j+1]
                +wz2*wr2*D->EzCR[m][i+1][j+1])*coss[m]
               -(wz1*wr2*D->EzCI[m][i][j+1]
                +wz2*wr2*D->EzCI[m][i+1][j+1])*sins[m]);
           Bz+=((wz1*wr2*D->BzCR[m][i][j+1]
                +wz2*wr2*D->BzCR[m][i+1][j+1])*coss[m]
               -(wz1*wr2*D->BzCI[m][i][j+1]
                +wz2*wr2*D->BzCI[m][i+1][j+1])*sins[m]);
           Pr+=((WZ1*WR1*D->PrCR[m][i1-1][j1-1]
                +WZ2*WR1*D->PrCR[m][i1][j1-1]
                +WZ1*WR2*D->PrCR[m][i1-1][j1]
                +WZ2*WR2*D->PrCR[m][i1][j1])*coss[m]
               -(WZ1*WR1*D->PrCI[m][i1-1][j1-1]
                +WZ2*WR1*D->PrCI[m][i1][j1-1]
                +WZ1*WR2*D->PrCI[m][i1-1][j1]
                +WZ2*WR2*D->PrCI[m][i1][j1])*sins[m]);
           Pl+=((WZ1*WR1*D->PlCR[m][i1-1][j1-1]
                +WZ2*WR1*D->PlCR[m][i1][j1-1]
                +WZ1*WR2*D->PlCR[m][i1-1][j1]
                +WZ2*WR2*D->PlCR[m][i1][j1])*coss[m]
               -(WZ1*WR1*D->PlCI[m][i1-1][j1-1]
                +WZ2*WR1*D->PlCI[m][i1][j1-1]
                +WZ1*WR2*D->PlCI[m][i1-1][j1]
                +WZ2*WR2*D->PlCI[m][i1][j1])*sins[m]);
           Sr+=((WZ1*WR1*D->SrCR[m][i1-1][j1-1]
                +WZ2*WR1*D->SrCR[m][i1][j1-1]
                +WZ1*WR2*D->SrCR[m][i1-1][j1]
                +WZ2*WR2*D->SrCR[m][i1][j1])*coss[m]
               -(WZ1*WR1*D->SrCI[m][i1-1][j1-1]
                +WZ2*WR1*D->SrCI[m][i1][j1-1]
                +WZ1*WR2*D->SrCI[m][i1-1][j1]
                +WZ2*WR2*D->SrCI[m][i1][j1])*sins[m]);
           Sl+=((WZ1*WR1*D->SlCR[m][i1-1][j1-1]
                +WZ2*WR1*D->SlCR[m][i1][j1-1]
                +WZ1*WR2*D->SlCR[m][i1-1][j1]
                +WZ2*WR2*D->SlCR[m][i1][j1])*coss[m]
               -(WZ1*WR1*D->SlCI[m][i1-1][j1-1]
                +WZ2*WR1*D->SlCI[m][i1][j1-1]
                +WZ1*WR2*D->SlCI[m][i1-1][j1]
                +WZ2*WR2*D->SlCI[m][i1][j1])*sins[m]);
 
           for(m=2; m<numMode; m++) {
             Ez+=((wz1*D->EzCR[m][i][j+1]
                  +wz2*D->EzCR[m][i+1][j+1])*coss[m]
                 -(wz1*D->EzCI[m][i][j+1]
                  +wz2*D->EzCI[m][i+1][j+1])*sins[m]);
             Bz+=((wz1*D->BzCR[m][i][j+1]
                  +wz2*D->BzCR[m][i+1][j+1])*coss[m]
                 -(wz1*D->BzCI[m][i][j+1]
                  +wz2*D->BzCI[m][i+1][j+1])*sins[m]);
             Pr+=((WZ1*WR2*D->PrCR[m][i1-1][j1]
                  +WZ2*WR2*D->PrCR[m][i1][j1])*coss[m]
                 -(WZ1*WR2*D->PrCI[m][i1-1][j1]
                  +WZ2*WR2*D->PrCI[m][i1][j1])*sins[m]);
             Pl+=((WZ1*WR2*D->PlCR[m][i1-1][j1]
                  +WZ2*WR2*D->PlCR[m][i1][j1])*coss[m]
                 -(WZ1*WR2*D->PlCI[m][i1-1][j1]
                  +WZ2*WR2*D->PlCI[m][i1][j1])*sins[m]);
             Sr+=((WZ1*WR2*D->SrCR[m][i1-1][j1]
                  +WZ2*WR2*D->SrCR[m][i1][j1])*coss[m]
                 -(WZ1*WR2*D->SrCI[m][i1-1][j1]
                  +WZ2*WR2*D->SrCI[m][i1][j1])*sins[m]);
             Sl+=((WZ1*WR2*D->SlCR[m][i1-1][j1]
                  +WZ2*WR2*D->SlCR[m][i1][j1])*coss[m]
                 -(WZ1*WR2*D->SlCI[m][i1-1][j1]
                  +WZ2*WR2*D->SlCI[m][i1][j1])*sins[m]);
           }
           Er=(Pr+Pl)*0.5; Bp=(Pr-Pl)*0.5;
           Ep=(Sl+Sr)*0.5; Br=(Sl-Sr)*0.5;
           Ex=Er*coss[1]-Ep*sins[1];
           Ey=Er*sins[1]+Ep*coss[1];
           Bx=Br*coss[1]-Bp*sins[1];
           By=Br*sins[1]+Bp*coss[1];

           p->Ez=Ez; p->Ex=Ex; p->Ey=Ey;
           p->Bz=Bz; p->Bx=Bx; p->By=By;

           p=p->next;
         }
       }		//for(s)        
     }		   //for(i,j)
}

void interpolation_Yee_1st(Domain *D,External *Ext)
{
   int i,j,m,i1,j1,istart,iend,jstart,jend,s,rank,numMode;
   double Bz,Br,Bp,Bx,By,Ez,Er,Ep,Ex,Ey,yR1,yR2,yI1,yI2;
   double coss[D->numMode],sins[D->numMode];
   double extE1,extE2,extE3,extB1,extB2,extB3,x,y,z,R,r,invR;
   double wz1,wz2,wr1,wr2,WZ1,WZ2,WR1,WR2;
   ptclList *p;
   int myrank, nprocs;    

   MPI_Comm_rank(MPI_COMM_WORLD, &myrank);

   istart=D->istart;   iend=D->iend;
   jstart=D->jstart;   jend=D->jend;
   numMode=D->numMode;

   Particle **particle;
   particle=D->particle;

   extE1=Ext->E1;   extE2=Ext->E2;
   extE3=Ext->E3;   extB1=Ext->B1;
   extB2=Ext->B2;   extB3=Ext->B3;

   for(i=istart; i<iend; i++)
     for(j=jstart+1; j<jend; j++) 
     { 
       for(s=0; s<D->nSpecies; s++)
       {
         p=particle[i][j].head[s]->pt;
         while(p) {
           z=p->z;  x=p->x; y=p->y;
           R=sqrt(x*x+y*y); invR=1.0/R;
           r=R+0.5-(j-jstart);

           wz2=z;             wz1=1.0-wz2;
           wr2=r;             wr1=1.0-wr2;
           i1=((int)(i+z+0.5));
           j1=((int)(j+r+0.5));
           WZ2=z+0.5-((int)(z+0.5));  WZ1=1.0-WZ2;
           WR2=r+0.5-((int)(r+0.5));  WR1=1.0-WR2;

           coss[0]=1.0; sins[0]=0.0;
           coss[1]=x*invR; sins[1]=y*invR;
           for(m=2; m<numMode; m++) {
             coss[m]=coss[m-1]*coss[1]-sins[m-1]*sins[1];
             sins[m]=sins[m-1]*coss[1]+coss[m-1]*sins[1];
           }

           Bz=wz1*WR1*D->BzNowR[0][i][j1-1]
             +wz2*WR1*D->BzNowR[0][i+1][j1-1]
             +wz1*WR2*D->BzNowR[0][i][j1]
             +wz2*WR2*D->BzNowR[0][i+1][j1];
           Br=WZ1*wr1*D->BrNowR[0][i1-1][j]
             +WZ2*wr1*D->BrNowR[0][i1][j]
             +WZ1*wr2*D->BrNowR[0][i1-1][j+1]
             +WZ2*wr2*D->BrNowR[0][i1][j+1];
           Bp=WZ1*WR1*D->BpNowR[0][i1-1][j1-1]
             +WZ2*WR1*D->BpNowR[0][i1][j1-1]
             +WZ1*WR2*D->BpNowR[0][i1-1][j1]
             +WZ2*WR2*D->BpNowR[0][i1][j1];
           Ez=WZ1*wr1*D->EzR[0][i1-1][j]
             +WZ2*wr1*D->EzR[0][i1][j]
             +WZ1*wr2*D->EzR[0][i1-1][j+1]
             +WZ2*wr2*D->EzR[0][i1][j+1];
           Er=wz1*WR1*D->ErR[0][i][j1-1]
             +wz2*WR1*D->ErR[0][i+1][j1-1]
             +wz1*WR2*D->ErR[0][i][j1]
             +wz2*WR2*D->ErR[0][i+1][j1];
           Ep=wz1*wr1*D->EpR[0][i][j]
             +wz2*wr1*D->EpR[0][i+1][j]
             +wz1*wr2*D->EpR[0][i][j+1]
             +wz2*wr2*D->EpR[0][i+1][j+1];

           for(m=1; m<numMode; m++) {
             Bz+=((wz1*WR1*D->BzNowR[m][i][j1-1]
                  +wz2*WR1*D->BzNowR[m][i+1][j1-1]
                  +wz1*WR2*D->BzNowR[m][i][j1]
                  +wz2*WR2*D->BzNowR[m][i+1][j1])*coss[m]
                 -(wz1*WR1*D->BzNowI[m][i][j1-1]
                  +wz2*WR1*D->BzNowI[m][i+1][j1-1]
                  +wz1*WR2*D->BzNowI[m][i][j1]
                  +wz2*WR2*D->BzNowI[m][i+1][j1])*sins[m]);
             Br+=((WZ1*wr1*D->BrNowR[m][i1-1][j]
                  +WZ2*wr1*D->BrNowR[m][i1][j]
                  +WZ1*wr2*D->BrNowR[m][i1-1][j+1]
                  +WZ2*wr2*D->BrNowR[m][i1][j+1])*coss[m]
                 -(WZ1*wr1*D->BrNowI[m][i1-1][j]
                  +WZ2*wr1*D->BrNowI[m][i1][j]
                  +WZ1*wr2*D->BrNowI[m][i1-1][j+1]
                  +WZ2*wr2*D->BrNowI[m][i1][j+1])*sins[m]);
             Bp+=((WZ1*WR1*D->BpNowR[m][i1-1][j1-1]
                  +WZ2*WR1*D->BpNowR[m][i1][j1-1]
                  +WZ1*WR2*D->BpNowR[m][i1-1][j1]
                  +WZ2*WR2*D->BpNowR[m][i1][j1])*coss[m]
                 -(WZ1*WR1*D->BpNowI[m][i1-1][j1-1]
                  +WZ2*WR1*D->BpNowI[m][i1][j1-1]
                  +WZ1*WR2*D->BpNowI[m][i1-1][j1]
                  +WZ2*WR2*D->BpNowI[m][i1][j1])*sins[m]);
             Ez+=((WZ1*wr1*D->EzR[m][i1-1][j]
                  +WZ2*wr1*D->EzR[m][i1][j]
                  +WZ1*wr2*D->EzR[m][i1-1][j+1]
                  +WZ2*wr2*D->EzR[m][i1][j+1])*coss[m]
                 -(WZ1*wr1*D->EzI[m][i1-1][j]
                  +WZ2*wr1*D->EzI[m][i1][j]
                  +WZ1*wr2*D->EzI[m][i1-1][j+1]
                  +WZ2*wr2*D->EzI[m][i1][j+1])*sins[m]);
             Er+=((wz1*WR1*D->ErR[m][i][j1-1]
                  +wz2*WR1*D->ErR[m][i+1][j1-1]
                  +wz1*WR2*D->ErR[m][i][j1]
                  +wz2*WR2*D->ErR[m][i+1][j1])*coss[m]
                 -(wz1*WR1*D->ErI[m][i][j1-1]
                  +wz2*WR1*D->ErI[m][i+1][j1-1]
                  +wz1*WR2*D->ErI[m][i][j1]
                  +wz2*WR2*D->ErI[m][i+1][j1])*sins[m]);
             Ep+=((wz1*wr1*D->EpR[m][i][j]
                  +wz2*wr1*D->EpR[m][i+1][j]
                  +wz1*wr2*D->EpR[m][i][j+1]
                  +wz2*wr2*D->EpR[m][i+1][j+1])*coss[m]
                 -(wz1*wr1*D->EpI[m][i][j]
                  +wz2*wr1*D->EpI[m][i+1][j]
                  +wz1*wr2*D->EpI[m][i][j+1]
                  +wz2*wr2*D->EpI[m][i+1][j+1])*sins[m]);
           }
           Ex=Er*coss[1]-Ep*sins[1];
           Ey=Er*sins[1]+Ep*coss[1];
           Bx=Br*coss[1]-Bp*sins[1];
           By=Br*sins[1]+Bp*coss[1];

           p->Ez=Ez; p->Ex=Ex; p->Ey=Ey;
           p->Bz=Bz; p->Bx=Bx; p->By=By;

           p=p->next;
         }
       }		//for(s)        
     }		   //for(i,j)

   j=jstart;
   for(i=istart; i<iend; i++)
     { 
       for(s=0; s<D->nSpecies; s++)
       {
         p=particle[i][j].head[s]->pt;
         while(p) {
           z=p->z;  x=p->x; y=p->y;
           R=sqrt(x*x+y*y); invR=1.0/R;
           r=R+0.5-(j-jstart);

           coss[0]=1.0; sins[0]=0.0;
           coss[1]=x*invR; sins[1]=y*invR;
           for(m=2; m<numMode; m++) {
             coss[m]=coss[m-1]*coss[1]-sins[m-1]*sins[1];
             sins[m]=sins[m-1]*coss[1]+coss[m-1]*sins[1];
           }

           wz2=z;             wz1=1.0-wz2;
           WZ2=z+0.5-((int)(z+0.5));  WZ1=1.0-WZ2;
           i1=((int)(i+z+0.5));

           wr2=(r-0.5)*2.0;             wr1=1.0-wr2;

           m=0;
           yR1=D->BrNowR[m][i1-1][j+1];
           yR2=D->BrNowR[m][i1][j+1];
           Br=WZ1*wr2*yR1+WZ2*wr2*yR2;
           yR1=D->EpR[m][i][j+1];
           yR2=D->EpR[m][i+1][j+1];
           Ep=wz1*wr2*yR1+wz2*wr2*yR2;
           yR1=D->EzR[m][i1-1][j+1];
           yR2=D->EzR[m][i1][j+1];
           Ez=WZ1*yR1+WZ2*yR2;
  
           m=1;
           yR1=D->BrNowR[m][i1-1][j+1];
           yR2=D->BrNowR[m][i1][j+1];
           yI1=D->BrNowI[m][i1-1][j+1];
           yI2=D->BrNowI[m][i1][j+1];
           Br+=(WZ1*yR1+WZ2*yR2)*coss[m]-(WZ1*yI1+WZ2*yI2)*sins[m];
           yR1=D->EpR[m][i][j+1];
           yR2=D->EpR[m][i+1][j+1];
           yI1=D->EpI[m][i][j+1];
           yI2=D->EpI[m][i+1][j+1];
           Ep+=(wz1*yR1+wz2*yR2)*coss[m]-(wz1*yI1+wz2*yI2)*sins[m];
           yR1=D->EzR[m][i1-1][j+1];
           yR2=D->EzR[m][i1][j+1];
           yI1=D->EzI[m][i1-1][j+1];
           yI2=D->EzI[m][i1][j+1];
           Ez+=wr2*(WZ1*yR1+WZ2*yR2)*coss[m]-wr2*(WZ1*yI1+WZ2*yI2)*sins[m];
 
           for(m=2; m<numMode; m++) {
             yR1=D->BrNowR[m][i1-1][j+1];
             yR2=D->BrNowR[m][i1][j+1];
             yI1=D->BrNowI[m][i1-1][j+1];
             yI2=D->BrNowI[m][i1][j+1];
             Br+=wr2*(WZ1*yR1+WZ2*yR2)*coss[m]-wr2*(WZ1*yI1+WZ2*yI2)*sins[m];
             yR1=D->EpR[m][i][j+1];
             yR2=D->EpR[m][i+1][j+1];
             yI1=D->EpI[m][i][j+1];
             yI2=D->EpI[m][i+1][j+1];
             Ep+=wr2*(wz1*yR1+wz2*yR2)*coss[m]-wr2*(wz1*yI1+wz2*yI2)*sins[m];
             yR1=D->EzR[m][i1-1][j+1];
             yR2=D->EzR[m][i1][j+1];
             yI1=D->EzI[m][i1-1][j+1];
             yI2=D->EzI[m][i1][j+1];
             Ez+=wr2*(WZ1*yR1+WZ2*yR2)*coss[m]-wr2*(WZ1*yI1+WZ2*yI2)*sins[m];
           }


           WR2=(r-0.5);             WR1=1.0-WR2;

           m=0;
           yR1=D->ErR[m][i][j+1];
           yR2=D->ErR[m][i+1][j+1];
           Er=WR2*wz1*yR1+WR2*wz2*yR2;
           Bz=WR2*wz1*D->BzNowR[m][i][j+1]
             +WR2*wz2*D->BzNowR[m][i+1][j+1]
             +WR1*wz1*D->BzNowR[m][i][j]
             +WR1*wz2*D->BzNowR[m][i+1][j];
           yR1=D->BpNowR[m][i1-1][j+1];
           yR2=D->BpNowR[m][i1][j+1];
           Bp=WR2*WZ1*yR1+WR2*WZ2*yR2;

           m=1;
           Er+=(WR1*wz1*D->ErR[m][i][j]
               +WR1*wz2*D->ErR[m][i+1][j]
               +WR2*wz1*D->ErR[m][i][j+1]
               +WR2*wz2*D->ErR[m][i+1][j+1])*coss[m]
              -(WR1*wz1*D->ErI[m][i][j]
               +WR1*wz2*D->ErI[m][i+1][j]
               +WR2*wz1*D->ErI[m][i][j+1]
               +WR2*wz2*D->ErI[m][i+1][j+1])*sins[m];
           yR1=D->BzNowR[m][i][j+1];
           yR2=D->BzNowR[m][i+1][j+1];
           yI1=D->BzNowI[m][i][j+1];
           yI2=D->BzNowI[m][i+1][j+1];
           Bz+=WR2*(wz1*yR1+wz2*yR2)*coss[m]-WR2*(wz1*yI1+wz2*yI2)*sins[m];
           Bp+=(WR1*WZ1*D->BpNowR[m][i1-1][j]
               +WR1*WZ2*D->BpNowR[m][i1][j]
               +WR2*WZ1*D->BpNowR[m][i1-1][j+1]
               +WR2*WZ2*D->BpNowR[m][i1][j+1])*coss[m]
              -(WR1*WZ1*D->BpNowI[m][i1-1][j]
               +WR1*WZ2*D->BpNowI[m][i1][j]
               +WR2*WZ1*D->BpNowI[m][i1-1][j+1]
               +WR2*WZ2*D->BpNowI[m][i1][j+1])*sins[m];

           for(m=2; m<numMode; m++) {
             yR1=D->ErR[m][i][j+1];
             yR2=D->ErR[m][i+1][j+1];
             yI1=D->ErI[m][i][j+1];
             yI2=D->ErI[m][i+1][j+1];
             Er+=WR2*(wz1*yR1+wz2*yR2)*coss[m]-WR2*(wz1*yI1+wz2*yI2)*sins[m];
             yR1=D->BzNowR[m][i][j+1];
             yR2=D->BzNowR[m][i+1][j+1];
             yI1=D->BzNowI[m][i][j+1];
             yI2=D->BzNowI[m][i+1][j+1];
             Bz+=WR2*(wz1*yR1+wz2*yR2)*coss[m]-WR2*(wz1*yI1+wz2*yI2)*sins[m];
             yR1=D->BpNowR[m][i1-1][j+1];
             yR2=D->BpNowR[m][i1][j+1];
             yI1=D->BpNowI[m][i1-1][j+1];
             yI2=D->BpNowI[m][i1][j+1];
             Bp+=WR2*(WZ1*yR1+WZ2*yR2)*coss[m]-WR2*(WZ1*yI1+WZ2*yI2)*sins[m];
           }
           Ex=Er*coss[1]-Ep*sins[1];
           Ey=Er*sins[1]+Ep*coss[1];
           Bx=Br*coss[1]-Bp*sins[1];
           By=Br*sins[1]+Bp*coss[1];

           p->Ez=Ez; p->Ex=Ex; p->Ey=Ey;
           p->Bz=Bz; p->Bx=Bx; p->By=By;

           p=p->next;
         }
       }		//for(s)        
     }		   //for(i,j)

